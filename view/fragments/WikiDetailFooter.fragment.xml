<core:FragmentDefinition	xmlns="sap.m" 
							xmlns:core="sap.ui.core">

	<OverflowToolbar>
		<ToolbarSpacer/>
		<ToolbarSpacer/>
		<Button id="btnNavUp" icon="sap-icon://navigation-up-arrow" text="{i18n>textBtnNavUp}" press="onNavToWiki"/>
		<Button id="btnNavLeft" icon="sap-icon://navigation-left-arrow" text="{ path: 'i18n>textBtnNavLeft', formatter:'.formatter.device' }" press="onNavToWikiDetailPrevious"/>
		<Button id="btnNavRight" icon="sap-icon://navigation-right-arrow" text="{ path: 'i18n>textBtnNavRight', formatter:'.formatter.device'}" press="onNavToWikiDetailNext"/>
	</OverflowToolbar>

</core:FragmentDefinition>